{
  "name": "nest-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:local": "NODE_ENV=development ENV_FILE=.env.local STAGE=dev nest start",
    "dev": "NODE_ENV=development ENV_FILE=.env.local STAGE=dev nest start --watch",
    "start:local:watch": "NODE_ENV=development ENV_FILE=.env.local STAGE=dev nest start --watch",
    "start:local:debug": "NODE_ENV=development ENV_FILE=.env.local STAGE=dev nest start --debug --watch",
    "start:dev": "NODE_ENV=development ENV_FILE=.env.development STAGE=dev node dist/main",
    "start:stg": "NODE_ENV=production ENV_FILE=.env.staging STAGE=stg node dist/main",
    "start:prod": "NODE_ENV=production ENV_FILE=.env.production STAGE=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.1.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.0",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/platform-express": "^10.1.0",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/websockets": "^10.1.0",
    "axios": "^0.26.0",
    "class-validator": "^0.13.2",
    "express-http-context": "^1.2.4",
    "helmet": "^4.6.0",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "shortid": "^2.2.16",
    "slugify": "^1.6.5",
    "uuidv4": "^6.2.12",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.2.5",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.7",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
